.container{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.singleBox{
  width: 95%;
  height: 84rpx;
  margin-top: 30rpx;
  border-bottom: 1px solid #e6e6e6;
}
.bigPart{
  display: inline-block;
  font-size: 48rpx;
  margin-left: 20rpx;
  color: #707070;
}
.smallPart{
  display: inline-block;
  margin-left: 20rpx;
  font-size: 28rpx;
  color: #a6a6a6;
}
.rightBox{
  position: relative;
  float: right;
  display: inline-block;
  height: 60rpx;
  width: 24rpx;
  font-size: 60rpx;
  line-height: 60rpx;
  color: #9e9e9e;
}
